<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" auto-import="true" assembly="WindowsApplication1" namespace="WindowsApplication1.BussinessObject.Common" default-lazy="false">
  <class name="PembelianBarang" table="pembelian_barang">
    <composite-id name="CompositeId" class="PembelianBarangId">
      <key-property name="eidpembelian" column="idpembelian" type="Int32">
        <generator class="foreign" />
      </key-property>
      <key-property name="eidbarang" column="idbarang" type="Int32">
        <generator class="foreign" />
      </key-property>
    </composite-id>
    <property name="ejumlah" type="Int32" column="jumlah" length="11"/>
    <property name="eharga" type="Decimal" column="harga"/>
    <property name="esubtotal" type="Decimal" column="subtotal"/>

    <many-to-one name="eidpembelianset" column="idpembelian" class="Pembelian"  not-null="true" />
    <many-to-one name="eidbarangset" column="idbarang" class="Barang"  not-null="true"/>

  </class>
</hibernate-mapping>